<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="helloworldtutorial.learn.com.helloworldtutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".KamalActivity"
            android:label="@string/Love_Kamal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Service-->
        <service android:name=".HelloWorldService"/>
        <!-- -->
        <receiver android:name=".HelloWorldBroadcastReciever">
            <intent-filter>
                <action android:name="CUTOM_INTENT"></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>
